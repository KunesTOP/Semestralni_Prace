@{
    ViewData["Title"] = "Funkce";
}

<h1>Zvířata majitele</h1>

@if (Model != null && Model.Any())
{
    <ul>
        @foreach (var animal in Model)
        {
            <li>@animal.Name (@animal.Age let)</li> <!-- Přizpůsobte podle vašeho modelu zvířat -->
        }
    </ul>
}
else
{
    <p>Žádná zvířata nebyla nalezena.</p>
}